<ejb-jar
	xmlns = "http://java.sun.com/xml/ns/javaee"
	version = "3.1"
	xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation = "http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">
	<interceptors>
		<interceptor>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor>
	</interceptors>
	
	<assembly-descriptor>		
		<interceptor-binding>
			<ejb-name>ChannelDaoBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>DNIDListDaoBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>MobileTerminalPluginDaoBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>OceanRegionDaoBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>PollDaoBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>PollProgramDaoBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>TerminalDaoBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>

	</assembly-descriptor>	
</ejb-jar>